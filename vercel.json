{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/",
      "destination": "/public/home.html",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/public/assets/:path*"
    },
    {
      "source": "/pages/:path*",
      "destination": "/public/pages/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/public/:path*"
    }
  ]
}
